	eprint       = {0901.4107},
	keywords     = {methods: numerical, galaxies: interactions, cosmology: dark matter},
	local-url    = {file://localhost/Users/phopkins/Documents/Papers/Springel/Springel_2009_E%20pur%20si%20muove%20Galiliean-invariant.pdf},
